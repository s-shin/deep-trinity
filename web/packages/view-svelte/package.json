    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@deep-trinity/model": "^0.1.0"
  },
  "devDependencies": {
    "@deep-trinity/web-core": "^0.1.0",

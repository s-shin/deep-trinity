    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@tsconfig/svelte": "^5.0.2",
    "svelte": "^4.2.0",
    "vite": "^4.4.9",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2"
  },
  "peerDependencies": {
    "svelte": "^4.2.0"
  }
}
